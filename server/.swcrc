{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "baseUrl": "./src",
    "paths": {
      "Utils/*": ["utils/*"],
      "Types/*": ["types/*"]
    },
    "transform": null,
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false
  },
  "module": {
    "type": "commonjs"
  }
}
