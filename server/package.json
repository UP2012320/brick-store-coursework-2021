{
  "name": "server",
  "scripts": {
    "start": "node dist/index.js",
    "build": "swc src -d dist",
    "serve": "npm run build && node dist/index.js",
    "watch": "nodemon --watch src --ext ts --exec \"npm run serve\"",
    "lint": "eslint --cache --fix -c .eslintrc.js src",
    "lint-no-fix": "eslint --cache -c .eslintrc.js src"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.172",
    "@types/node": "^17.0.29",
    "@types/pg": "^8.6.5",
    "@types/sharp": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "api-types": "workspace:^1.0.0",
    "eslint": "^8.14.0",
    "eslint-config-canonical": "^34.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "fastify": "^3.28.0",
    "fastify-auth0-verify": "^0.7.3",
    "fastify-cors": "^6.0.3",
    "fastify-helmet": "^7.0.1",
    "fastify-multipart": "^5.3.1",
    "fastify-postgres": "^3.6.0",
    "fastify-sensible": "^3.1.2",
    "fastify-static": "^4.6.1",
    "graphql": "^16.4.0",
    "nanoid": "^3.3.3",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "sharp": "^0.30.4",
    "slugify": "^1.6.5",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "graphql": "^16.4.0"
  }
}
